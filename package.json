{
  "name": "kelseyleaphotography",
  "version": "0.0.1",
  "scripts": {
    "start": "vite dev --port 3000",
    "build": "vite build",
    "build:prod": "(sleep 30 && vite build)",
    "postbuild": "svelte-sitemap --domain https://kelseyleaphotography.com -o .vercel_build_output/static",
    "preview": "vite preview",
    "build:sanity": "cd studio && npm run build",
    "start:sanity": "cd studio && npm run start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@portabletext/svelte": "^1.0.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint": "^8.16.0",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier": "^2.6.2",
    "svelte-check": "^2.7.1",
    "svelte-sitemap": "^2.5.1",
    "svelte": "^3.44.0",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@sanity/client": "^3.4.1",
    "@sanity/image-url": "^1.0.1"
  }
}
